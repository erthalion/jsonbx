CREATE EXTENSION jsonbx;
select jsonb_pretty('{"a": "test", "b": [1, 2, 3], "c": "test3", "d":{"dd": "test4", "dd2":{"ddd": "test5"}}}'::jsonb);
        jsonb_pretty        
----------------------------
 {                         +
     "a": "test",          +
     "b":                  +
     [                     +
         1,                +
         2,                +
         3                 +
     ],                    +
     "c": "test3",         +
     "d":                  +
     {                     +
         "dd": "test4",    +
         "dd2":            +
         {                 +
             "ddd": "test5"+
         }                 +
     }                     +
 }
(1 row)

